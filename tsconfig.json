{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": "src",
        "sourceMap": false,
        "declaration": false,
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "typeRoots": [
            "node_modules/@types"
        ]
    },
    "exclude": [
        "node_modules",
        "src/**/*.spec.ts",
        "test/*"
    ]
}
